{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://raw.githubusercontent.com/charbonnierg/pytest-discover/main/schemas/test_case_setup.json",
  "title": "Test Case Setup",
  "description": "Pytest Test Case Setup",
  "type": "object",
  "required": [
    "node_id",
    "event_type",
    "outcome",
    "duration",
    "start",
    "stop"
  ],
  "properties": {
    "node_id": {
      "type": "string",
      "description": "Node ID"
    },
    "event_type": {
      "const": "case_setup",
      "description": "Event Type"
    },
    "outcome": {
      "$ref": "https://raw.githubusercontent.com/charbonnierg/pytest-discover/main/schemas/outcome.json",
      "description": "Outcome of the setup step"
    },
    "duration": {
      "type": "number",
      "description": "Duration of the setup step in seconds"
    },
    "start": {
      "type": "string",
      "format": "date-time",
      "description": "Start time of the setup step in ISO 8601 format"
    },
    "stop": {
      "type": "string",
      "format": "date-time",
      "description": "Stop time of the setup step in ISO 8601 format"
    },
    "error": {
      "$ref": "https://raw.githubusercontent.com/charbonnierg/pytest-discover/main/schemas/test_case_error.json",
      "description": "Error details if the setup step failed"
    }
  }
}