{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "https://raw.githubusercontent.com/charbonnierg/pytest-discover/main/schemas/test_case_setup.json",
  "title": "Test Case Setup",
  "description": "Pytest Test Case Setup",
  "type": "object",
  "required": [
    "node_id",
    "event_type",
    "outcome",
    "duration"
  ],
  "properties": {
    "node_id": {
      "type": "string",
      "description": "Node ID"
    },
    "event_type": {
      "const": "case_setup",
      "description": "Event Type"
    },
    "outcome": {
      "$ref": "https://raw.githubusercontent.com/charbonnierg/pytest-discover/main/schemas/outcome.json"
    },
    "duration": {
      "type": "number",
      "description": "Duration in seconds"
    }
  }
}