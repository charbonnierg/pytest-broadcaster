{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "test_case_outcome.json",
  "title": "Test Case Finished",
  "description": "Pytest Test Case Finished",
  "type": "object",
  "required": [
    "node_id",
    "event_type",
    "outcome"
  ],
  "properties": {
    "node_id": {
      "type": "string",
      "description": "Node ID"
    },
    "event_type": {
      "const": "case_finished",
      "description": "Event Type"
    },
    "outcome": {
      "$ref": "outcome.json/#"
    }
  }
}