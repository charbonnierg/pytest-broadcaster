{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "test_case_call.json",
  "title": "Test Case Call",
  "description": "Pytest Test Case Call",
  "type": "object",
  "required": [
    "node_id",
    "event_type",
    "outcome",
    "duration"
  ],
  "properties": {
    "node_id": {
      "type": "string",
      "description": "Node ID"
    },
    "event_type": {
      "const": "case_call",
      "description": "Event Type"
    },
    "outcome": {
      "$ref": "outcome.json/#"
    },
    "duration": {
      "type": "number",
      "description": "Duration in seconds"
    }
  }
}