{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "test_case_error.json",
  "title": "Test Case Error",
  "description": "Pytest Test Case Error",
  "type": "object",
  "required": [
    "message"
  ],
  "properties": {
    "message": {
      "type": "string",
      "description": "Error message"
    },
    "traceback": {
      "$ref": "traceback.json/#",
      "description": "Error traceback"
    }
  }
}